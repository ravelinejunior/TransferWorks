<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activities.usuario.RequisicoesMotoristaActivity">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerView_requisicoesMotorista"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mensagem_Requisicao_motorista"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:text="@string/aguardando_requsicoes"
        app:layout_constraintBottom_toBottomOf="@+id/recyclerView_requisicoesMotorista"
        app:layout_constraintEnd_toEndOf="@+id/recyclerView_requisicoesMotorista"
        app:layout_constraintStart_toStartOf="@+id/recyclerView_requisicoesMotorista"
        app:layout_constraintTop_toTopOf="@+id/recyclerView_requisicoesMotorista" />
</androidx.constraintlayout.widget.ConstraintLayout>